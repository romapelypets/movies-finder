{
  "name": "movies-finder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve -o",
    "prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run prod && firebase deploy",
    "format:fix": "pretty-quick --staged",
    "ssr": "npm run build:universal && npm run generate:prerender && npm run server",
    "server": "node dist/server",
    "generate:prerender": "node dist/prerender.js",
    "build": "ng build",
    "build:server": "ng run movies-finder:server:production",
    "build:universal": "npm run prod && ng run movies-finder:server:production && webpack --config webpack.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.12",
    "@angular/common": "~7.2.9",
    "@angular/compiler": "~7.2.9",
    "@angular/core": "~7.2.9",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "~7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/platform-browser": "~7.2.9",
    "@angular/platform-browser-dynamic": "~7.2.9",
    "@angular/platform-server": "^7.2.9",
    "@angular/pwa": "^0.13.6",
    "@angular/router": "~7.2.9",
    "@angular/service-worker": "~7.2.9",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-meta/core": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "firebase": "^5.9.3",
    "firebase-functions": "^2.2.0",
    "husky": "^1.3.1",
    "karma-spec-reporter": "0.0.32",
    "ngx-toastr": "^10.0.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "pretty-quick": "^1.10.0",
    "rxjs": "~6.4.0",
    "rxjs-tslint": "^0.1.7",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.3",
    "webpack-cli": "^3.3.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.9",
    "@angular/language-service": "~7.2.9",
    "@types/jasmine": "~3.3.10",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^11.11.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "1.16.4",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.3",
    "tslint": "~5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  }
